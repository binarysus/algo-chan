{
  "name": "algo-chan",
  "version": "0.1.0",
  "description": "a collaborative Discord bot for the binarysus server",
  "main": "./dist/index.js",
  "scripts": {
    "build": "rm -rf dist && npx tsc",
    "start": "npx env-cmd node .",
    "lint": "npx eslint src",
    "flint": "npx eslint src --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/binarysus/algo-chan.git"
  },
  "author": "itsUrcute",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/binarysus/algo-chan/issues"
  },
  "homepage": "https://github.com/binarysus/algo-chan#readme",
  "imports": {
    "#commands/*": "./dist/commands/*.js",
    "#constants/*": "./dist/constants/*.js",
    "events/*": "./dist/events/*.js",
    "#internals/*": "./dist/internals/*.js",
    "#types/*": "./dist/types/*.js",
    "#utils/*": "./dist/utils/*.js"
  },
  "dependencies": {
    "color-convert": "^2.0.1",
    "discord.js": "^13.0.1",
    "env-cmd": "^10.1.0",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@types/color-convert": "^2.0.0",
    "@types/node-fetch": "^2.5.11",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "eslint-config-marine": "^9.0.6",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5"
  }
}
